{
  "name": "rcn.io",
  "version": "0.0.1",
  "description": "Source code for rcn.io, project that makes cycling in USA awesome.",
  "scripts": {
    "clean:client": "rimraf dist",
    "clean:server": "rimraf dist-server",
    "clean:all": "npm run clean:client && npm run clean:server && rimraf node_modules",
    "build:webpack-client": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js --progress --bail",
    "build:dll": "webpack --config webpack.config.dev.dll.js --progress",
    "build:webpack-server": "cross-env NODE_ENV=production webpack --config webpack.config.server.prod.js --progress --bail",
    "build:client": "npm run clean:client && npm run build:webpack-client",
    "build:server": "npm run clean:server && npm run build:webpack-server",
    "build": "concurrently \"npm run build:client\" \"npm run build:server\"",
    "start": "cross-env NODE_ENV=production NODE_PATH=./src node src/server/index.js",
    "dev:server": "cross-env NODE_PATH=./src nodemon src/server/index.js",
    "dev:server-debug": "npm run dev:server --inspect --debug-brk",
    "dev": "npm run build:dll && node src/server/devServer.js",
    "eslint:ci": "eslint src/ --ext .js,.jsx --max-warnings 10",
    "eslint": "npm run eslint:ci || true",
    "eslint:fix": "eslint src/ --ext .js,.jsx --fix || true",
    "reinstall": "npm run clean:all && npm i",
    "postinstall": "npm run build",
    "ngrok": "ngrok http 8888",
    "tape-tests": "cross-env NODE_PATH=./src tape -r './src/tests/index.js' 'src/**/tests/**/*.js'",
    "test:one": "cross-env NODE_PATH=./src tape -r './src/tests/index.js'",
    "test": "npm run tape-tests | tap-spec",
    "test:dot": "npm run tape-tests | tap-dot || true",
    "events-converter:dev": "cross-env NODE_ENV=development NODE_PATH=./src nodemon src/server/events/usac/events-converter",
    "events-converter": "cross-env NODE_ENV=production NODE_PATH=./src node src/server/events/usac/events-converter"
  },
  "repository": {
    "url": "https://github.com/Restuta/rcn.io.git",
    "type": "git"
  },
  "private": true,
  "keywords": [
    "rcn",
    "bike",
    "cycling",
    "racing"
  ],
  "author": "Anton Vynogradenko <restuta8@gmail.com> (http://restuta.me)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Restuta/rcn.io/issues"
  },
  "homepage": "http://rcn.io",
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "aws-sdk": "^2.9.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-react-constant-elements": "^6.8.0",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2015-node6": "^0.3.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.17.0",
    "babel-register": "^6.24.1",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "concurrently": "^2.1.0",
    "cross-env": "^5.0.5",
    "css-loader": "0.14.5",
    "eslint": "~2.11.1",
    "eslint-plugin-react": "^5.1.1",
    "express": "^4.13.3",
    "express-device": "^0.4.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "imports-loader": "^0.6.5",
    "joi": "^10.6.0",
    "json-loader": "^0.5.4",
    "leven": "^2.1.0",
    "mime-types": "^2.1.12",
    "morgan": "^1.7.0",
    "ms": "^0.7.1",
    "node-noop": "^1.0.0",
    "node-sass": "^3.7.0",
    "nodemon": "^1.11.0",
    "noop-loader": "^1.0.0",
    "object-assign": "^4.1.0",
    "postcss-loader": "^0.10.1",
    "prettier": "^1.5.3",
    "react-addons-perf": "^15.4.2",
