{
  "name": "rcn.io",
  "version": "0.0.1",
  "description": "Source code for rcn.io, project that makes cycling in USA awesome.",
  "scripts": {
    "clean:client": "rimraf dist",
    "clean:server": "rimraf dist-server",
    "clean:all": "npm run clean:client && npm run clean:server && rimraf bower_components && rimraf node_modules",
    "build:webpack-client": "NODE_ENV=production webpack -p --config webpack.config.prod.js --progress --bail",
    "build:webpack-server": "NODE_ENV=production webpack -p --config webpack.config.server.prod.js --progress --bail",
    "build:client": "npm run clean:client && npm run build:webpack-client",
    "build:server": "npm run clean:server && npm run build:webpack-server",
    "build": "npm run clean:client && npm run clean:server && concurrently \"npm run build:client\" \"npm run build:server\"",
    "start": "NODE_ENV=production node src/server/index.js",
    "dev:server": "nodemon src/server/index.js",
    "dev": "node src/server/devServer.js",
    "eslint:ci": "eslint src/ --ext .js,.jsx",
    "eslint": "eslint src/ --ext .js,.jsx || true",
    "eslint:fix": "eslint src/ --ext .js,.jsx --fix || true",
    "reinstall": "npm run clean:all && bower i && npm i",
    "postinstall": "bower install && npm run build",
    "ngrok": "ngrok http 8888"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Restuta/rcn.io.git"
  },
  "private": true,
  "keywords": [
    "rcn",
    "bike",
    "cycling",
    "racing"
  ],
  "author": "Anton Vynogradenko <restuta8@gmail.com> (http://restuta.me)",
  "license": "",
  "bugs": {
    "url": "https://github.com/Restuta/rcn.io/issues"
  },
  "homepage": "http://rcn.io",
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-core": "^6.7.2",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-react-constant-elements": "^6.5.0",
    "babel-plugin-transform-react-inline-elements": "^6.6.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-node5": "^1.2.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "bower": "^1.7.9",
    "compression": "^1.6.1",
    "concurrently": "^2.0.0",
    "css-loader": "0.14.5",
    "eslint": "~2.2.0",
    "eslint-plugin-react": "^4.1.0",
    "express": "^4.13.3",
    "express-device": "^0.4.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.15.0",
    "imports-loader": "^0.6.5",
    "mime-types": "^2.1.11",
    "morgan": "^1.7.0",
    "ms": "^0.7.1",
    "node-noop": "^1.0.0",
    "node-sass": "^3.3.3",
    "noop-loader": "^1.0.0",
    "postcss-loader": "^0.8.2",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.2",
    "redbox-react": "^1.2.2",
    "rimraf": "^2.4.4",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.0",
    "svg-inline-loader": "^0.4.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-node-externals": "^1.0.0"
  },
  "dependencies": {
    "classnames": "^2.2.1",
    "moment": "^2.11.2",
    "moment-timezone": "^0.5.4",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-pure-render": "^1.0.1",
    "react-router": "^2.4.0",
    "svg-inline-react": "^1.0.0"
  },
  "engines": {
    "node": "5.10.x"
  }
}
